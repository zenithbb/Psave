{
    "identifier": "default",
    "description": "Capability for the main window",
    "windows": [
        "main"
    ],
    "permissions": [
        "shell:allow-open",
        {
            "identifier": "shell:allow-execute",
            "allow": [
                {
                    "name": "cargo",
                    "cmd": "cargo",
                    "args": true
                }
            ]
        }
    ]
}